        BUN mm;                   /* # tuples / values in tmp arrays */
        BUN f, l;                 /* first / last BUN per slice */
	pthread_t *s_Thread;      /* threads array */
        s_Thread_t *s_Thread_arg; /* thread arguments array */
        int i;
#ifdef SCAN_BREAKDOWN
        FILE *ofp;
        char *outputFilename1 = "/export/scratch2/petraki/experiments_sigmod_2014/parallel_scan_analysis/parallelscan_breakdown.txt";
        lng t0, t1;
