str
CRKparallelscan_@2_@1( BAT *b, BAT *ob, @1 mval, BUN first, BUN last, int nthreads){
	str msg = MAL_SUCCEED;
	assert(b && ob);
	assert(last >= first);

	msg = CRKscan_MT_@2_@1(b, ob, first, last, mval, nthreads);

